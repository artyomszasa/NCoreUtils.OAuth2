<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <AspNetCoreHostingModel>OutOfProcess</AspNetCoreHostingModel>
    <ServerGarbageCollection>false</ServerGarbageCollection>
    <RetainVMGarbageCollection>false</RetainVMGarbageCollection>
    <TargetLatestRuntimePatch>true</TargetLatestRuntimePatch>
    <PublishReadyToRun>true</PublishReadyToRun>
    <PublishReadyToRunShowWarnings>true</PublishReadyToRunShowWarnings>
    <!--
    <PublishTrimmed>true</PublishTrimmed>
    -->
    <Version>2.0.0-alpha4</Version>
    <NoWarn>FS2003</NoWarn>
    <IsPackable>false</IsPackable>
  </PropertyGroup>
  <ItemGroup>
    <TrimmerRootAssembly Include="System.Threading.Tasks" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="Config.fs" />
    <Compile Include="ServiceCollectionCustomRestExtensions.fs" />
    <Compile Include="Startup.fs" />
    <Compile Include="Program.fs" />
  </ItemGroup>
  <ItemGroup Condition=" '$(Configuration)' == 'Debug' ">
    <PackageReference Include="NCoreUtils.XUnit.Abstractions" Version="3.0.0-preview8" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Update="FSharp.Core" Version="4.7.0" />
    <PackageReference Include="Microsoft.NETCore.Platforms" Version="3.0.0-preview8.19405.3" />
    <PackageReference Include="NCoreUtils.AspNetCore.FileUploader" Version="3.0.0-preview8.1" />
    <PackageReference Include="NCoreUtils.AspNetCore.Rest" Version="3.0.0-preview8.1" />
    <PackageReference Include="NCoreUtils.ContentDetection" Version="3.0.0-preview8" />
    <PackageReference Include="NCoreUtils.ContentDetection.Magic.Libmagic" Version="3.0.0-preview8" />
    <PackageReference Include="NCoreUtils.Data.Abstractions" Version="3.0.0-preview8.1" />
    <PackageReference Include="NCoreUtils.Data.Protocol" Version="3.0.0-preview8.1" />
    <PackageReference Include="NCoreUtils.FSharp" Version="3.0.0-preview8.1" />
    <PackageReference Include="NCoreUtils.FSharp.AspNetCore" Version="3.0.0-preview8.1" />
    <PackageReference Include="NCoreUtils.FSharp.DependencyInjection" Version="3.0.0-preview8.1" />
    <PackageReference Include="NCoreUtils.Images.Client" Version="2.9.0-alpha3" />
    <PackageReference Include="NCoreUtils.Images.DependencyInjection" Version="2.9.0-alpha3" />
    <PackageReference Include="NCoreUtils.Storage.Abstractions" Version="3.0.0-preview8" />
    <PackageReference Include="NCoreUtils.Storage.FileSystem" Version="3.0.0-preview8" />
    <PackageReference Include="NCoreUtils.Text" Version="1.1.0" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.2" />
    <PackageReference Include="System.Collections.Immutable" Version="1.6.0-preview8.19405.3" />
    <PackageReference Include="System.Net.Requests" Version="4.3.0" />
    <PackageReference Include="System.Net.Http" Version="4.3.4" />
    <PackageReference Include="System.Net.WebHeaderCollection" Version="4.3.0" />
    <PackageReference Include="System.Runtime.Loader" Version="4.3.0" />
    <PackageReference Include="System.Threading.Tasks.Parallel" Version="4.3.0" />
    <PackageReference Include="System.Threading.Thread" Version="4.3.0" />
    <PackageReference Include="System.Threading.ThreadPool" Version="4.3.0" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\NCoreUtils.OAuth2.Core\NCoreUtils.OAuth2.Core.fsproj" />
    <ProjectReference Include="..\NCoreUtils.OAuth2.Data.EntityFrameworkCore\NCoreUtils.OAuth2.Data.EntityFrameworkCore.csproj" />
    <ProjectReference Include="..\NCoreUtils.OAuth2.Authentication\NCoreUtils.OAuth2.Authentication.fsproj" />
    <ProjectReference Include="..\NCoreUtils.OAuth2.Middleware\NCoreUtils.OAuth2.Middleware.fsproj" />
    <ProjectReference Include="..\NCoreUtils.OAuth2.Rest\NCoreUtils.OAuth2.Rest.fsproj" />
    <ProjectReference Include="..\NCoreUtils.OAuth2.Encryption.Local\NCoreUtils.OAuth2.Encryption.Local.fsproj" />
  </ItemGroup>
